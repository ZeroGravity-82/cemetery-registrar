<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <entity name="Cemetery\Registrar\Domain\NaturalPerson\NaturalPerson" table="natural_person">
        <id name="id" column="id" type="natural_person_id">
        </id>

        <field name="fullName"  column="full_name"  type="full_name"          nullable="true" />
        <field name="bornAt"    column="born_at"    type="date_immutable"     nullable="true" />
        <field name="createdAt" column="created_at" type="datetime_immutable"                 />
        <field name="updatedAt" column="updated_at" type="datetime_immutable"                 />
        <field name="removedAt" column="removed_at" type="datetime_immutable" nullable="true" />
    </entity>

</doctrine-mapping>
