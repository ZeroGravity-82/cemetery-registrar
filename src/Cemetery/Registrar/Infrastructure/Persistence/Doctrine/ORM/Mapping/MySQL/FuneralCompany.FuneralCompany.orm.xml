<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <entity name="Cemetery\Registrar\Domain\FuneralCompany\FuneralCompany" table="funeral_company">
        <id name="id" column="id" type="funeral_company_id">
        </id>

        <field name="note"      column="note"       type="string"             nullable="true" />
        <field name="createdAt" column="created_at" type="datetime_immutable"                 />
        <field name="updatedAt" column="updated_at" type="datetime_immutable"                 />

        <embedded name="organizationId" class="Cemetery\Registrar\Domain\Organization\OrganizationId" use-column-prefix="false" />
    </entity>

</doctrine-mapping>
