<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <embeddable name="Cemetery\Registrar\Domain\GeoPosition\Coordinates">
        <field name="latitude"  column="geo_latitude"  type="decimal" precision="10" scale="8" nullable="true" />
        <field name="longitude" column="geo_longitude" type="decimal" precision="11" scale="8" nullable="true" />
    </embeddable>

</doctrine-mapping>
