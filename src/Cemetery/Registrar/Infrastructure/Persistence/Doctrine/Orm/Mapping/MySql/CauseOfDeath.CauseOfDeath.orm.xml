<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <entity name="Cemetery\Registrar\Domain\Model\CauseOfDeath\CauseOfDeath" table="cause_of_death">
        <id name="id" column="id" type="cause_of_death_id" />

        <field name="name"        column="name"        type="cause_of_death_name"                 />
        <field name="createdAt"   column="created_at"  type="datetime_immutable"                  />
        <field name="updatedAt"   column="updated_at"  type="datetime_immutable"                  />
        <field name="removedAt"   column="removed_at"  type="datetime_immutable"  nullable="true" />
    </entity>

</doctrine-mapping>
