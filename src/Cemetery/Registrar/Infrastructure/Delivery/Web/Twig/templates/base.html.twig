<!doctype html>
<html lang="ru">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" href="/favicon.ico">

    <link href="//cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link href="//cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css" rel="stylesheet">
    <link href="/css/offcanvas.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet">
    <link href="/css/libs/selectize/standalone/selectize.css" rel="stylesheet">
    <link href="/css/libs/selectize/standalone/selectize.bootstrap5.css" rel="stylesheet">
    {% block stylesheets %}{% endblock %}

    <title>{% block title %}{% endblock %}</title>
  </head>
  <body>
    {% include '_spinner.html.twig' %}
    {% include '_flash_message.html.twig' %}
    <main class="container-fluid no-select">
      {% block header %}
        {% include '_navbar.html.twig' %}
      {% endblock %}
      {% block body %}{% endblock %}
    </main>

    <script src="//cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
    <script src="//code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="/js/offcanvas.js"></script>
    <script src="/js/main.js"></script>
    <script src="/js/libs/sweetalert2/sweetalert2.all.min.js"></script>
    <script src="/js/libs/selectize/standalone/selectize.min.js"></script>
    {% block javascripts %}{% endblock %}
  </body>
</html>
