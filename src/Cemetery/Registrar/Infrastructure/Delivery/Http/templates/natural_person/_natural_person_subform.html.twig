<div class="js-natural-person-subform d-none">
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}NaturalPersonFullName" class="form-label">Ф.И.О.</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}NaturalPersonFullName" name="{{- context -}}NaturalPersonFullName"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}NaturalPersonPhone" class="form-label">Телефон</label></div>
    <div class="col-md-9"><input type="tel" class="form-control form-control-sm" id="{{- context -}}NaturalPersonPhone" name="{{- context -}}NaturalPersonPhone"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}NaturalPersonPhoneAdditional" class="form-label">Доп. телефон</label></div>
    <div class="col-md-9"><input type="tel" class="form-control form-control-sm" id="{{- context -}}NaturalPersonPhoneAdditional" name="{{- context -}}NaturalPersonPhoneAdditional"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}NaturalPersonAddress" class="form-label">Адрес</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}NaturalPersonAddress" name="{{- context -}}NaturalPersonAddress"></div>
  </div>
  <p class="mt-1 mb-0">
    <a class="fw-bold" type="button" data-bs-toggle="collapse" data-bs-target="#{{- context -}}NaturalPersonPassport" aria-expanded="false" aria-controls="{{- context -}}NaturalPersonPassport">
      Паспортные данные
    </a>
  </p>
  <div class="collapse" id="{{- context -}}NaturalPersonPassport">
    {% include 'natural_person/_passport_subform.html.twig' with {'context': context} %}
  </div>
</div>
