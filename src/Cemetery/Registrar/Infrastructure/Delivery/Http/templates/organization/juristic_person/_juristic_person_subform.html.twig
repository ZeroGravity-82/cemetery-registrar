<div class="js-juristic-person-subform d-none">
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonName" class="form-label">Наименование</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonName" name="{{- context -}}JuristicPersonName"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonInn" class="form-label">ИНН</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonInn" name="{{- context -}}JuristicPersonInn"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonKpp" class="form-label">КПП</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonKpp" name="{{- context -}}JuristicPersonKpp"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonOgrn" class="form-label">ОГРН</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonOgrn" name="{{- context -}}JuristicPersonOgrn"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonOkpo" class="form-label">ОКПО</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonOkpo" name="{{- context -}}JuristicPersonOkpo"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonOkved" class="form-label">ОКВЭД</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonOkved" name="{{- context -}}JuristicPersonOkved"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonLegalAddress" class="form-label">Юридический адрес</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonLegalAddress" name="{{- context -}}JuristicPersonLegalAddress"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonPostalAddress" class="form-label">Почтовый адрес</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonPostalAddress" name="{{- context -}}JuristicPersonPostalAddress"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonPhone" class="form-label">Телефон</label></div>
    <div class="col-md-9"><input type="tel" class="form-control form-control-sm" id="{{- context -}}JuristicPersonPhone" name="{{- context -}}JuristicPersonPhone"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonPhoneAdditional" class="form-label"></label>Телефон доп.</div>
    <div class="col-md-9"><input type="tel" class="form-control form-control-sm" id="{{- context -}}JuristicPersonPhoneAdditional" name="{{- context -}}JuristicPersonPhoneAdditional"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonFax" class="form-label">Факс</label></div>
    <div class="col-md-9"><input type="tel" class="form-control form-control-sm" id="{{- context -}}JuristicPersonFax" name="{{- context -}}JuristicPersonFax"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonGeneralDirector" class="form-label">Генеральный директор</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonGeneralDirector" name="{{- context -}}JuristicPersonGeneralDirector"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonEmail" class="form-label">Электронная почта</label></div>
    <div class="col-md-9"><input type="email" class="form-control form-control-sm" id="{{- context -}}JuristicPersonEmail" name="{{- context -}}JuristicPersonEmail"></div>
  </div>
  <div class="row">
    <div class="col-md-3"><label for="{{- context -}}JuristicPersonWebsite" class="form-label">Веб-сайт</label></div>
    <div class="col-md-9"><input type="text" class="form-control form-control-sm" id="{{- context -}}JuristicPersonWebsite" name="{{- context -}}JuristicPersonWebsite"></div>
  </div>
  <p class="mt-1 mb-1">
    <a class="fw-bold" type="button" data-bs-toggle="collapse" data-bs-target="#{{- context -}}JuristicPersonBankDetails" aria-expanded="false" aria-controls="{{- context -}}JuristicPersonBankDetails">
      Банковские реквизиты
    </a>
  </p>
  <div class="collapse" id="{{- context -}}JuristicPersonBankDetails">
    {% include 'organization/bank_details/_bank_details_subform.html.twig' with {
      'context'         : context,
      'organizationType': 'juristicPerson'
    } %}
  </div>
</div>
